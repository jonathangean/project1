
<h2>Welcome</h2>

<% if current_user %>
  <p>
    <%= current_user.username %>
  </p>
  <%= link_to "Log-Out", log_out_path, method: :delete %>
<% else%>
  <%= link_to "Log-In", log_in_path %>
  <%= link_to "Sign-Up", new_user_path %>
<% end %>

<ul>
  <h1>Users</h1>
<% @users.each do |user| %>
  <li>
    <a href="/users/<%= user.id %>"><%= user.username %></a>
  </li>
  <% end %>
  </ul>
